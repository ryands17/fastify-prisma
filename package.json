{
  "name": "fastify-prisma",
  "version": "1.0.0",
  "description": "Basic implementation of Prisma with Fastify",
  "main": "index.js",
  "private": true,
  "scripts": {
    "dev": "nodemon src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "dotenv -e .env.test -- jest",
    "db:save": "prisma migrate dev --create-only --preview-feature",
    "db:migrate": "prisma migrate dev --preview-feature",
    "db:migrate:prod": "prisma migrate deploy --preview-feature",
    "db:seed": "prisma db seed --preview-feature",
    "postinstall": "husky install"
  },
  "keywords": [],
  "author": "Ryan",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "3.1.1",
    "bcrypt": "5.0.1",
    "fastify": "3.29.5",
    "fastify-cors": "6.0.2",
    "fastify-helmet": "5.3.2",
    "jsonwebtoken": "8.5.1"
  },
  "devDependencies": {
    "@types/bcrypt": "10.0.28-alpha",
    "@types/jest": "27.0.2",
    "@types/jsonwebtoken": "8.5.5",
    "@types/node": "16.10.2",
    "@types/nodemon": "1.19.1",
    "@types/prettier": "2.4.1",
    "@types/supertest": "2.0.11",
    "dotenv-cli": "4.0.0",
    "husky": "7.0.2",
    "jest": "27.2.4",
    "jest-environment-node": "27.2.4",
    "lint-staged": "11.2.0",
    "nodemon": "2.0.13",
    "prettier": "2.4.1",
    "prisma": "3.1.1",
    "supertest": "6.1.6",
    "ts-eager": "2.0.2",
    "ts-jest": "27.0.5",
    "ts-node": "10.2.1",
    "typescript": "4.4.3",
    "typesync": "0.8.0",
    "unique-names-generator": "4.6.0"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "arrowParens": "avoid"
  },
  "lint-staged": {
    "*.{ts,js,json,md}": "prettier --write"
  }
}
