{
  "name": "fastify-prisma",
  "version": "1.0.0",
  "description": "Basic implementation of Prisma with Fastify",
  "main": "index.js",
  "private": true,
  "scripts": {
    "dev": "nodemon src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "dotenv -e .env.test -- jest",
    "db:save": "prisma migrate dev --create-only --preview-feature",
    "db:migrate": "prisma migrate dev --preview-feature",
    "db:migrate:prod": "prisma migrate deploy --preview-feature",
    "db:seed": "prisma db seed --preview-feature",
    "postinstall": "husky install"
  },
  "keywords": [],
  "author": "Ryan",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "2.21.2",
    "bcrypt": "5.0.1",
    "fastify": "3.15.1",
    "fastify-cors": "6.0.1",
    "fastify-helmet": "5.3.1",
    "jsonwebtoken": "8.5.1"
  },
  "devDependencies": {
    "@types/bcrypt": "10.0.28-alpha",
    "@types/jsonwebtoken": "8.5.1",
    "@types/node": "14.14.43",
    "@types/nodemon": "1.19.0",
    "@types/prettier": "2.2.3",
    "@types/supertest": "2.0.10",
    "dotenv-cli": "4.0.0",
    "husky": "6.0.0",
    "jest": "26.6.3",
    "jest-environment-node": "26.6.2",
    "lint-staged": "10.5.4",
    "nodemon": "2.0.7",
    "prettier": "2.2.1",
    "prisma": "2.21.2",
    "supertest": "6.1.3",
    "ts-eager": "1.1.3",
    "ts-jest": "26.5.2",
    "ts-node": "9.1.1",
    "typescript": "4.2.4",
    "typesync": "0.8.0",
    "unique-names-generator": "4.4.0"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "arrowParens": "avoid"
  },
  "lint-staged": {
    "*.{ts,js,json,md}": "prettier --write"
  }
}
