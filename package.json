{
  "name": "fastify-prisma",
  "version": "1.0.0",
  "description": "Basic implementation of Prisma with Fastify",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "jest",
    "db:save": "prisma migrate dev --create-only --preview-feature",
    "db:migrate": "prisma migrate dev --preview-feature",
    "db:migrate:prod": "prisma migrate deploy --preview-feature",
    "db:seed": "prisma db seed --preview-feature"
  },
  "keywords": [],
  "author": "Ryan",
  "license": "ISC",
  "dependencies": {
    "@prisma/cli": "2.15.0",
    "@prisma/client": "2.15.0",
    "fastify": "3.12.0",
    "fastify-cors": "^5.1.0",
    "fastify-helmet": "^5.1.0"
  },
  "devDependencies": {
    "@types/node": "14.14.28",
    "@types/nodemon": "1.19.0",
    "@types/prettier": "2.2.1",
    "@types/supertest": "2.0.10",
    "husky": "4.3.8",
    "jest": "26.6.3",
    "jest-environment-node": "26.6.2",
    "lint-staged": "10.5.4",
    "nodemon": "2.0.7",
    "prettier": "2.2.1",
    "supertest": "6.1.3",
    "ts-eager": "1.1.3",
    "ts-jest": "26.4.4",
    "ts-node": "9.1.1",
    "typescript": "4.1.3",
    "typesync": "0.8.0",
    "unique-names-generator": "4.3.1"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "arrowParens": "avoid"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,js,json,md}": "prettier --write"
  }
}
